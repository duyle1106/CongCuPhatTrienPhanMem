[32m+[m[32m            if (!string.IsNullOrEmpty(searchString))[m
[32m+[m[32m            {[m
[32m+[m[32m                model = model.Where(x => x.ID.Contains(searchString) || x.Content.Contains(searchString)).OrderByDescending(x=>x.ID);[m
[32m+[m[32m            }[m
[32m+[m[32m            return model.OrderByDescending(X => X.ID).ToPagedList(page, pageSize);[m
[32m+[m[32m        }[m
[32m+[m[32m        public Footer GetFooter()[m
[32m+[m[32m        {[m
[32m+[m[32m            return db.Footer.SingleOrDefault(x => x.Status == true);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
