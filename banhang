[1mdiff --git a/WebBanHang/WebBanHang/WebBanHang/banhang/Model/D/AboutD.cs b/WebBanHang/WebBanHang/WebBanHang/banhang/Model/D/AboutD.cs[m
[1mnew file mode 100644[m
[1mindex 0000000..f262152[m
[1m--- /dev/null[m
[1m+++ b/WebBanHang/WebBanHang/WebBanHang/banhang/Model/D/AboutD.cs[m
[36m@@ -0,0 +1,88 @@[m
[32m+[m[32m﻿using Model.D;[m
[32m+[m[32musing Model.EF;[m
[32m+[m[32musing PagedList;[m
[32m+[m[32musing System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.Linq;[m
[32m+[m[32musing System.Text;[m
[32m+[m[32musing System.Threading.Tasks;[m
[32m+[m
[32m+[m[32mnamespace Model.D[m
[32m+[m[32m{[m
[32m+[m[32m    public class AboutD[m
[32m+[m[32m    {[m
[32m+[m[32m        OnlineShopDbContext db = null;[m
[32m+[m[32m        public AboutD()[m
[32m+[m[32m        {[m
[32m+[m[32m            db = new OnlineShopDbContext();[m
[32m+[m[32m        }[m
[32m+[m[32m        public List<About> ListAll()[m
[32m+[m[32m        {[m
[32m+[m[32m            return db.About.Where(x => x.Status == true).ToList();[m
[32m+[m[32m        }[m
[32m+[m[32m        public About ViewDetail(long id)[m
[32m+[m[32m        {[m
[32m+[m[32m            return db.About.Find(id);[m
[32m+[m[32m        }[m
[32m+[m[32m        public long Insert(About entity)[m
[32m+[m[32m        {[m
[32m+[m[32m            db.About.Add(entity);[m
[32m+[m[32m            db.SaveChanges();[m
[32m+[m[32m            return entity.ID;[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m        public bool Update(About entity)[m
[32m+[m[32m        {[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                var about = db.About.Find(entity.ID);[m
[32m+[m[32m                about.Name = entity.Name;[m
[32m+[m[32m                about.Image = entity.Image;[m
[32m+[m[32m                about.MetaTitle = entity.MetaTitle;[m
[32m+[m[32m                about.ModifiedBy = entity.ModifiedBy;[m
[32m+[m[32m                about.ModifiedDate = DateTime.Now;[m
[32m+[m[32m                db.SaveChanges();[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Exception ex)[m
[32m+[m[32m            {[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        public IEnumerable<About> ListAllPaging(string searchString, int page, int pageSize)[m
[32m+[m[32m        {[m
[32m+[m[32m            IQueryable<About> model = db.About;[m
[32m+[m[32m            if (!string.IsNullOrEmpty(searchString))[m
[32m+[m[32m            {[m
[32m+[m[32m                model = model.Where(x => x.Name.Contains(searchString) || x.MetaTitle.Contains(searchString)).OrderByDescending(x => x.CreatedDate);[m
[32m+[m[32m            }[m
[32m+[m[32m            return model.OrderByDescending(X => X.CreatedDate).ToPagedList(page, pageSize);[m
[32m+[m[32m        }[m
[32m+[m[32m        public About GetById(string name)[m
[32m+[m[32m        {[m
