[32m+[m[32m        public virtual DbSet<Footer> Footer { get; set; }[m
[32m+[m[32m        public virtual DbSet<Menu> Menu { get; set; }[m
[32m+[m[32m        public virtual DbSet<MenuType> MenuType { get; set; }[m
[32m+[m[32m        public virtual DbSet<Order> Order { get; set; }[m
[32m+[m[32m        public virtual DbSet<OrderDetail> OrderDetail { get; set; }[m
[32m+[m[32m        public virtual DbSet<Product> Product { get; set; }[m
[32m+[m[32m        public virtual DbSet<ProductCategory> ProductCategory { get; set; }[m
[32m+[m[32m        public virtual DbSet<Slide> Slide { get; set; }[m
[32m+[m[32m        public virtual DbSet<sysdiagrams> sysdiagrams { get; set; }[m
[32m+[m[32m        public virtual DbSet<SystemConfig> SystemConfig { get; set; }[m
[32m+[m[32m        public virtual DbSet<Tag> Tag { get; set; }[m
[32m+[m[32m        public virtual DbSet<User> User { get; set; }[m
[32m+[m
[32m+[m[32m        public virtual DbSet<Role> Role { set; get; }[m
[32m+[m[32m        public virtual DbSet<UserGroup> UserGroup { set; get; }[m
[32m+[m[32m        public virtual DbSet<Credential> Credential { set; get; }[m
[32m+[m
[32m+[m[32m        protected override void OnModelCreating(DbModelBuilder modelBuilder)[m
[32m+[m[32m        {[m
[32m+[m[32m            modelBuilder.Entity<About>()[m
[32m+[m[32m                .Property(e => e.MetaTitle)[m
[32m+[m[32m                .IsUnicode(false);[m
[32m+[m
[32m+[m[32m            modelBuilder.Entity<About>()[m
[32m+[m[32m                .Property(e => e.CreatedBy)[m
[32m+[m[32m                .IsUnicode(false);[m
[32m+[m
[32m+[m[32m            modelBuilder.Entity<About>()[m
[32m+[m[32m                .Property(e => e.ModifiedBy)[m
[32m+[m[32m                .IsUnicode(false);[m
[32m+[m
[32m+[m[32m            modelBuilder.Entity<About>()[m
[32m+[m[32m                .Property(e => e.MetaDescriptions)[m
[32m+[m[32m                .IsFixedLength();[m
[32m+[m
