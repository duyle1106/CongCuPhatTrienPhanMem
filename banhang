[32m+[m[32m        }[m
[32m+[m[32m        public Content GetByID(long id)[m
[32m+[m[32m        {[m
[32m+[m[32m            return db.Content.Find(id);[m
[32m+[m[32m        }[m
[32m+[m[32m        public long Insert(Content entity)[m
[32m+[m[32m        {[m
[32m+[m[32m            db.Content.Add(entity);[m
[32m+[m[32m            db.SaveChanges();[m
[32m+[m[32m            return entity.ID;[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m        public bool Update(Content entity)[m
[32m+[m[32m        {[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                var content = db.Content.Find(entity.ID);[m
[32m+[m[32m                content.Name = entity.Name;[m
[32m+[m[32m                content.CategoryID = entity.CategoryID;[m
[32m+[m[32m                content.Image = entity.Image;[m
[32m+[m[32m                content.Detail = entity.Detail;[m
[32m+[m[32m                content.MetaTitle = entity.MetaTitle;[m
[32m+[m[32m                content.ModifiedBy = entity.ModifiedBy;[m
[32m+[m[32m                content.ModifiedDate = DateTime.Now;[m
[32m+[m[32m                db.SaveChanges();[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Exception ex)[m
[32m+[m[32m            {[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        public IEnumerable<Content> ListAllPaging(string searchString, int page, int pageSize)[m
[32m+[m[32m        {[m
[32m+[m[32m            IQueryable<Content> model = db.Content;[m
[32m+[m[32m            if (!string.IsNullOrEmpty(searchString))[m
[32m+[m[32m            {[m
[32m+[m[32m                model = model.Where(x => x.Name.Contains(searchString) || x.MetaTitle.Contains(searchString)).OrderByDescending(x => x.CreatedDate);[m
[32m+[m[32m            }[m
[32m+[m[32m            return model.OrderByDescending(X => X.CreatedDate).ToPagedList(page, pageSize);[m
[32m+[m[32m        }[m
[32m+[m[32m        public IEnumerable<Content> ListAllByTag(string tag, int page, int pageSize)[m
[32m+[m[32m        {[m
[32m+[m[32m            var model = (from a in db.Content[m
[32m+[m[32m                         join b in db.ContentTag[m
[32m+[m[32m                         on a.ID equals b.ContentID[m
[32m+[m[32m                         where b.TagID == tag[m
[32m+[m[32m                         select new[m
[32m+[m[32m                         {[m
[32m+[m[32m                             Name = a.Name,[m
[32m+[m[32m                             MetaTitle = a.MetaTitle,[m
[32m+[m[32m                             Image = a.Image,[m
[32m+[m[32m                             Description = a.Description,[m
[32m+[m[32m                             CreatedDate = a.CreatedDate,[m
[32m+[m[32m                             CreatedBy = a.CreatedBy,[m
[32m+[m[32m                             ID = a.ID[m
[32m+[m
[32m+[m[32m                         }).AsEnumerable().Select(x => new Content()[m
[32m+[m[32m                         {[m
[32m+[m[32m                             Name = x.Name,[m
[32m+[m[32m                             MetaTitle = x.MetaTitle,[m
[32m+[m[32m                             Image = x.Image,[m
[32m+[m[32m                             Description = x.Description,[m
[32m+[m[32m                             CreatedDate = x.CreatedDate,[m
[32m+[m[32m                             CreatedBy = x.CreatedBy,[m
[32m+[m[32m                             ID = x.ID[m
[32m+[m[32m                         });[m
[32m+[m[32m            return model.OrderByDescending(x => x.CreatedDate).ToPagedList(page, pageSize);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public Tag GetTag(string id)[m
[32m+[m[32m        {[m
[32m+[m[32m            return db.Tag.Find(id);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public Content ViewDetail(long id)[m
[32m+[m[32m        {[m
[32m+[m[32m            return db.Content.Find(id);[m
[32m+[m[32m        }[m
[32m+[m[32m        public bool Delete(int id)[m
[32m+[m[32m        {[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                var content = db.Content.Find(id);[m
[32m+[m[32m                db.Content.Remove(content);[m
[32m+[m[32m                db.SaveChanges();[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Exception)[m
[32m+[m[32m            {[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m        public bool ChangeStatus(long id)[m
[32m+[m[32m        {[m
[32m+[m[32m            var cont = db.Content.Find(id);[m
[32m+[m[32m            cont.Status = !cont.Status;[m
[32m+[m[32m            db.SaveChanges();[m
[32m+[m[32m            return cont.Status;[m
[32m+[m[32m        }[m
