[1mdiff --git a/WebBanHang/WebBanHang/WebBanHang/banhang/CommonConstants/MailHelper.cs b/WebBanHang/WebBanHang/WebBanHang/banhang/CommonConstants/MailHelper.cs[m
[1mnew file mode 100644[m
[1mindex 0000000..42ed0f7[m
[1m--- /dev/null[m
[1m+++ b/WebBanHang/WebBanHang/WebBanHang/banhang/CommonConstants/MailHelper.cs[m
[36m@@ -0,0 +1,38 @@[m
[32m+[m[32m﻿using System;[m
[32m+[m[32musing System.Collections.Generic;[m
[32m+[m[32musing System.Configuration;[m
[32m+[m[32musing System.Linq;[m
[32m+[m[32musing System.Net;[m
[32m+[m[32musing System.Net.Mail;[m
[32m+[m[32musing System.Text;[m
[32m+[m[32musing System.Threading.Tasks;[m
[32m+[m
[32m+[m[32mnamespace CommonConstants[m
[32m+[m[32m{[m
[32m+[m[32m    public class MailHelper[m
[32m+[m[32m    {[m
[32m+[m[32m        public void SendMail(string toEmailAddress, string subject, string content)[m
[32m+[m[32m        {[m
[32m+[m[32m            var fromEmailAddress = ConfigurationManager.AppSettings["FromEmailAddress"].ToString();[m
[32m+[m[32m            var fromEmailDisplayName = ConfigurationManager.AppSettings["FromEmailDisplayName"].ToString();[m
[32m+[m[32m            var fromEmailPassword = ConfigurationManager.AppSettings["FromEmailPassword"].ToString();[m
[32m+[m[32m            var smtpHost = ConfigurationManager.AppSettings["SMTPHost"].ToString();[m
[32m+[m[32m            var smtpPort = ConfigurationManager.AppSettings["SMTPPort"].ToString();[m
[32m+[m
[32m+[m[32m            bool enabledSsl = bool.Parse(ConfigurationManager.AppSettings["EnabledSSL"].ToString());[m
[32m+[m
[32m+[m[32m            string body = content;[m
[32m+[m[32m            MailMessage message = new MailMessage(new MailAddress(fromEmailAddress, fromEmailDisplayName), new MailAddress(toEmailAddress));[m
[32m+[m[32m            message.Subject = subject;[m
[32m+[m[32m            message.IsBodyHtml = true;[m
[32m+[m[32m            message.Body = body;[m
[32m+[m
[32m+[m[32m            var client = new SmtpClient();[m
[32m+[m[32m            client.Credentials = new NetworkCredential(fromEmailAddress, fromEmailPassword);[m
[32m+[m[32m            client.Host = smtpHost;[m
[32m+[m[32m            client.EnableSsl = enabledSsl;[m
[32m+[m[32m            client.Port = !string.IsNullOrEmpty(smtpPort) ? Convert.ToInt32(smtpPort) : 0;[m
[32m+[m[32m            client.Send(message);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
