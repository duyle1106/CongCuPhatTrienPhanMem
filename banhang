[32m+[m[32mnamespace Model.EF[m
[32m+[m[32m{[m
[32m+[m[32m    using System;[m
[32m+[m[32m    using System.Data.Entity;[m
[32m+[m[32m    using System.ComponentModel.DataAnnotations.Schema;[m
[32m+[m[32m    using System.Linq;[m
[32m+[m
[32m+[m[32m    public partial class OnlineShopDbContext : DbContext[m
[32m+[m[32m    {[m
[32m+[m[32m        public OnlineShopDbContext()[m
[32m+[m[32m            : base("name=OnlineShopDbContext")[m
[32m+[m[32m        {[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public virtual DbSet<About> About { get; set; }[m
[32m+[m[32m        public virtual DbSet<Category> Category { get; set; }[m
[32m+[m[32m        public virtual DbSet<Contact> Contact { get; set; }[m
