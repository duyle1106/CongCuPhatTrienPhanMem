[32m+[m[32m        }[m
[32m+[m[32m        public bool Update(Slide entity)[m
[32m+[m[32m        {[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                var slide = db.Slide.Find(entity.ID);[m
[32m+[m[32m                slide.Image= entity.Image;[m
[32m+[m[32m                slide.Link = entity.Link;[m
[32m+[m[32m                slide.DisplayOrder = entity.DisplayOrder;[m
[32m+[m[32m                slide.Description = entity.Description;[m
[32m+[m[32m                db.SaveChanges();[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
