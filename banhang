[32m+[m[32m        public long Insert(User entity)[m
[32m+[m[32m        {[m
[32m+[m[32m            db.User.Add(entity);[m
[32m+[m[32m            db.SaveChanges();[m
[32m+[m[32m            return entity.ID;[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[32m        public bool Update(User entity)[m
[32m+[m[32m        {[m
[32m+[m[32m            try[m
[32m+[m[32m            {[m
[32m+[m[32m                var user = db.User.Find(entity.ID);[m
[32m+[m[32m                user.Name = entity.Name;[m
[32m+[m[32m                if (!string.IsNullOrEmpty(entity.Password))[m
[32m+[m[32m                {[m
[32m+[m[32m                    user.Password = entity.Password;[m
[32m+[m[32m                }[m
[32m+[m[32m                user.Address = entity.Address;[m
[32m+[m[32m                user.Email = entity.Email;[m
[32m+[m[32m                user.Phone = entity.Phone;[m
[32m+[m[32m                user.GroupID = entity.GroupID;[m
[32m+[m[32m                user.ModifiedBy = entity.ModifiedBy;[m
[32m+[m[32m                user.ModifiedDate = DateTime.Now;[m
[32m+[m[32m                db.SaveChanges();[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m            catch(Exception ex)[m
[32m+[m[32m            {[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        public IEnumerable<User> ListAllPaging(string searchString, int page, int PageSize)[m
[32m+[m[32m        {[m
[32m+[m[32m            IQueryable<User> model = db.User;[m
[32m+[m[32m            if (!string.IsNullOrEmpty(searchString))[m
[32m+[m[32m            {[m
[32m+[m[32m                model = model.Where(x => x.UserName.Contains(searchString) || x.Name.Contains(searchString)).OrderByDescending(x=>x.CreatedDate);[m
[32m+[m[32m            }[m
[32m+[m[32m            return model.OrderByDescending(x => x.CreatedDate).ToPagedList(page, PageSize);[m
[32m+[m[32m        }[m
[32m+[m[32m        public User GetById(string userName)[m
[32m+[m[32m        {[m
