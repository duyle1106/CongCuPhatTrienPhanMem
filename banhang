[32m+[m[32m                product.ModifiedDate = DateTime.Now;[m
[32m+[m[32m                db.SaveChanges();[m
[32m+[m[32m                return true;[m
[32m+[m[32m            }[m
[32m+[m[32m            catch (Exception ex)[m
[32m+[m[32m            {[m
[32m+[m[32m                return false;[m
[32m+[m[32m            }[m
