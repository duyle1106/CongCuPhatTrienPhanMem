[32m+[m[32m        public bool ChangeStatus(long id)[m
[32m+[m[32m        {[m
[32m+[m[32m            var product = db.Product.Find(id);[m
[32m+[m[32m            product.Status = !product.Status;[m
[32m+[m[32m            db.SaveChanges();[m
[32m+[m[32m            return product.Status;[m
[32m+[m[32m        }[m
[32m+[m[32m        public List<Product> ListNewProduct(int top)[m
[32m+[m[32m        {[m
[32m+[m[32m            return db.Product.OrderByDescending(x => x.CreatedDate).Take(top).ToList();[m
[32m+[m[32m        }[m
[32m+[m[32m        public List<Product> ListFeatureProduct(int top)[m
[32m+[m[32m        {[m
[32m+[m[32m            return db.Product.Where(x => x.TopHot != null && x.TopHot > DateTime.Now).OrderByDescending(x => x.CreatedDate).Take(top).ToList();[m
[32m+[m[32m        }[m
[32m+[m[32m        public List<Product> ListRelatedProducts(long productId)[m
[32m+[m[32m        {[m
[32m+[m[32m            var product = db.Product.Find(productId);[m
[32m+[m[32m            return db.Product.Where(x => x.ID != productId && x.CategoryID == product.CategoryID).ToList();[m
[32m+[m[32m        }[m
[32m+[m[32m        public List<ProductViewModel> ListByCategoryId(long categoryID,ref int totalRecord,int pageIndex = 1,int pageSize = 2)[m
[32m+[m[32m        {[m
[32m+[m[32m            totalRecord = db.Product.Where(x => x.CategoryID == categoryID).Count();[m
[32m+[m[32m            var model = from a in db.Product[m
[32m+[m[32m                         join b in db.ProductCategory[m
[32m+[m[32m                         on a.CategoryID equals b.ID[m
[32m+[m[32m                         where a.CategoryID == categoryID[m
[32m+[m[32m                         select new ProductViewModel()[m
[32m+[m[32m                         {[m
[32m+[m[32m                             CateMetaTitle = b.MetaTitle,[m
[32m+[m[32m                             CateName = b.Name,[m
[32m+[m[32m                             CreatedDate = a.CreatedDate,[m
[32m+[m[32m                             ID = a.ID,[m
[32m+[m[32m                             Images = a.Image,[m
[32m+[m[32m                             Name = a.Name,[m
[32m+[m[32m                             MetaTitle = a.MetaTitle,[m
[32m+[m[32m                             Price = a.Price[m
[32m+[m[32m                         };[m
[32m+[m[32m            model.OrderByDescending(x => x.CreatedDate).Skip((pageIndex - 1) * pageSize).Take(pageSize);[m
[32m+[m[32m            return model.ToList();[m
[32m+[m[32m        }[m
[32m+[m[32m        public List<ProductViewModel> Search(string keyword, ref int totalRecord, int pageIndex = 1, int pageSize = 2)[m
[32m+[m[32m        {[m
[32m+[m[32m            totalRecord = db.Product.Where(x => x.Name == keyword).Count();[m
[32m+[m[32m            var model = (from a in db.Product[m
[32m+[m[32m                         join b in db.ProductCategory[m
[32m+[m[32m                         on a.CategoryID equals b.ID[m
[32m+[m[32m                         where a.Name.Contains(keyword)[m
[32m+[m[32m                         select new[m
[32m+[m[32m                         {[m
[32m+[m[32m                             CateMetaTitle = b.MetaTitle,[m
[32m+[m[32m                             CateName = b.Name,[m
[32m+[m[32m                             CreatedDate = a.CreatedDate,[m
[32m+[m[32m                             ID = a.ID,[m
[32m+[m[32m                             Images = a.Image,[m
[32m+[m[32m                             Name = a.Name,[m
[32m+[m[32m                             MetaTitle = a.MetaTitle,[m
[32m+[m[32m                             Price = a.Price[m
[32m+[m[32m                         }).AsEnumerable().Select(x => new ProductViewModel()[m
[32m+[m[32m                         {[m
[32m+[m[32m                             CateMetaTitle = x.MetaTitle,[m
[32m+[m[32m                             CateName = x.Name,[m
[32m+[m[32m                             CreatedDate = x.CreatedDate,[m
[32m+[m[32m                             ID = x.ID,[m
[32m+[m[32m                             Images = x.Images,[m
[32m+[m[32m                             Name = x.Name,[m
[32m+[m[32m                             MetaTitle = x.MetaTitle,[m
[32m+[m[32m                             Price = x.Price[m
[32m+[m[32m                         });[m
[32m+[m[32m            model.OrderByDescending(x => x.CreatedDate).Skip((pageIndex - 1) * pageSize).Take(pageSize);[m
[32m+[m[32m            return model.ToList();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
