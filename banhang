[32m+[m[32m        public IEnumerable<Product> ListAllPaging(string searchString, int page, int pageSize)[m
[32m+[m[32m        {[m
[32m+[m[32m            IQueryable<Product> model = db.Product;[m
[32m+[m[32m            if (!string.IsNullOrEmpty(searchString))[m
[32m+[m[32m            {[m
