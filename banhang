[32m+[m[32m        }[m
[32m+[m[32m        public User ViewDetail(long id)[m
[32m+[m[32m        {[m
[32m+[m[32m            return db.User.Find(id);[m
[32m+[m[32m        }[m
[32m+[m[32m        public List<string> GetListCreadential(string userName)[m
[32m+[m[32m        {[m
[32m+[m[32m            var user = db.User.Single(x => x.UserName == userName);[m
[32m+[m[32m            var data = (from a in db.Credential[m
[32m+[m[32m                        join b in db.UserGroup on a.UserGroupID equals b.ID[m
[32m+[m[32m                        join c in db.Role on a.RoleID equals c.ID[m
[32m+[m[32m                        where b.ID == user.GroupID[m
[32m+[m[32m                        select new[m[41m [m
[32m+[m[32m                        {[m
[32m+[m[32m                            RoleID = a.RoleID,[m
[32m+[m[32m                            UserGroupID = a.UserGroupID[m
[32m+[m
[32m+[m[32m                        }).AsEnumerable().Select(x => new Credential()[m
[32m+[m[32m                        {[m
[32m+[m[32m                            RoleID = x.RoleID,[m
[32m+[m[32m                            UserGroupID = x.UserGroupID[m
[32m+[m[32m                        });[m
[32m+[m[32m            return data.Select(x => x.RoleID).ToList();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        public int login(string userName, string passWord, bool isLoginAdmin = false)[m
[32m+[m[32m        {[m
[32m+[m[32m            var result = db.User.SingleOrDefault(x => x.UserName == userName);[m
[32m+[m[32m            if (result == null)[m
[32m+[m[32m            {[m
[32m+[m[32m                return 0;[m
[32m+[m[32m            }[m
[32m+[m[32m            else[m
[32m+[m[32m            {[m
[32m+[m[32m                if (isLoginAdmin == true)[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (result.GroupID == CommonConstants.CommonConstants.ADMIN_GROUP || result.GroupID == CommonConstants.CommonConstants.MOD_GROUP)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if (result.Status == false)[m
[32m+[m[32m                        {[m
[32m+[m[32m                            return -1;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        else[m
[32m+[m[32m                        {[m
[32m+[m[32m                            if (result.Password == passWord)[m
[32m+[m[32m                                return 1;[m
[32m+[m[32m                            else[m
[32m+[m[32m                                return -2;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m[41m                [m
[32m+[m[32m                else[m
[32m+[m[32m                {[m
[32m+[m[32m                    return -3;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m                else[m
[32m+[m[32m                {[m
[32m+[m[32m                    if (result.Status == false)[m
[32m+[m[32m                    {[m
[32m+[m[32m                        return -1;[m
[32m+[m[32m                    }[m
[32m+[m[32m                    else[m
[32m+[m[32m                    {[m
[32m+[m[32m                        if (result.Password == passWord)[m
[32m+[m[32m                            return 1;[m
[32m+[m[32m                        else[m
[32m+[m[32m                            return -2;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[41m        [m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        public bool ChangeStatus(long id)[m
[32m+[m[32m        {[m
[32m+[m[32m            var user = db.User.Find(id);[m
[32m+[m[32m            user.Status = !user.Status;[m
[32m+[m[32m            db.SaveChanges();[m
[32m+[m[32m            return user.Status;[m
[32m+[m[32m        }[m
[32m+[m[32m        public bool Delete(int id)[m
[32m+[m[32m        {[m
