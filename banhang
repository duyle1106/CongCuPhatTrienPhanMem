[32m+[m[32m        public List<string> ListName(string keyword)[m
[32m+[m[32m        {[m
[32m+[m[32m            return db.Product.Where(x => x.Name.Contains(keyword)).Select(x => x.Name).ToList();[m
[32m+[m[32m        }[m
[32m+[m[32m        public bool Update(Product entity)[m
