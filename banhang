[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        public IEnumerable<Slide> ListAllPaging(string searchString, int page, int pageSize)[m
[32m+[m[32m        {[m
[32m+[m[32m            IQueryable<Slide> model = db.Slide;[m
[32m+[m[32m            if (!string.IsNullOrEmpty(searchString))[m
[32m+[m[32m            {[m
[32m+[m[32m                model = model.Where(x => x.Link.Contains(searchString) || x.Link.Contains(searchString)).OrderByDescending(x => x.ModifiedDate);[m
[32m+[m[32m                }[m
[32m+[m[32m            return model.OrderByDescending(X => X.ModifiedDate).ToPagedList(page, pageSize);[m
[32m+[m[32m        }[m
[32m+[m[32m        public Slide GetById(string name)[m
[32m+[m[32m        {[m
